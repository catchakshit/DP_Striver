#include<bits/stdc++.h>
using namespace std;
// Time complexity O(n) Space complexity O(1);
int f(int n){
int prev2=0;
int prev=1;
int current;
for(int i=2;i<=n;i++){
	current =prev2 +prev;
	prev2=prev;
	prev=current;
	
}	

	
	
return current;
}

int main(){
	
int n;
cin>>n;
cout<<f(n);
return 0;	
}
